{
  "name": "projectile-CLI",
  "version": "1.0.0",
  "description": "booking hours for projectile",
  "main": "projectileCli.js",
  "scripts": {
    "build": "rm -rf src/api/public/api/doc && apidoc -v -p true -i src/api/ -o src/api/public/api/doc/",
    "api": "node src/api/app.js",
    "cleanup": "rm -f logs/*",
    "start": "node api.js",
    "start+debug": "node api.js debug",
    "lint": "./node_modules/.bin/eslint *.js",
    "prepush": "yarn lint",
    "test": "mocha --exit ./test"
  },
  "author": "Hai Dang",
  "contributors": {
    "name": "Christoph Bussenius"
  },
  "license": "ISC",
  "bin": {
    "api": "./api.js"
  },
  "dependencies": {
    "atob": "^2.1.1",
    "body-parser": "^1.18.2",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.13.0",
    "config": "^1.31.0",
    "encoding": "^0.1.12",
    "express": "^4.16.2",
    "express-router": "^0.0.1",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "sinon": "^6.1.0",
    "winston": "^3.0.0"
  },
  "pkg": {
    "assets": "src/**/*"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "mocha": "^5.2.0"
  }
