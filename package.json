{
  "name": "projectile-CLI",
  "version": "1.0.0",
  "description": "booking hours for projectile",
  "main": "projectileCli.js",
  "scripts": {
    "buildApi": "rm -rf src/api/public/api/doc && apidoc -v -p true -i src/api/ -o src/api/public/api/doc/",
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/api/app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "api": "node src/api/app.js",
    "cleanup": "rm -f logs/*",
    "start": "node src/api/app.js",
    "start+debug": "node api.js debug",
    "lint": "./node_modules/.bin/eslint **/*.js",
    "prepush": "yarn lint",
    "test": "mocha --exit ./test"
  },
  "author": "Hai Dang",
  "contributors": {
    "name": "Christoph Bussenius"
  },
  "license": "ISC",
  "bin": {
    "api": "./api.js"
  },
  "dependencies": {
    "atob": "^2.1.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.13.0",
    "config": "^1.31.0",
    "encoding": "^0.1.12",
    "express": "^4.16.2",
    "express-router": "^0.0.1",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "sinon": "^6.1.0",
    "winston": "^3.0.0"
  },
  "pkg": {
    "assets": "src/**/*"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3",
    "husky": "^0.14.3"
  }
}
